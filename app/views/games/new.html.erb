<%= form_for Game.new, url: {action: "create"} do |f| %>
  <%= f.label "Name" %><br>
  <%= f.text_field :name %><br>
  <%= f.label "Password" %><br>
  <%= f.text_field :password %><br>
  <%= f.label "Max points" %><br>
  <%= f.text_field :max_points %><br>
  <%= f.submit "Create Game" %><br>
<% end %>
